<odoo>
    <record id="view_product_pricelist_form_inherit_logs" model="ir.ui.view">
        <field name="name">product.pricelist.form.inherit.logs</field>
        <field name="model">product.pricelist</field>
        <field name="inherit_id" ref="product.product_pricelist_view"/> 
        <field name="arch" type="xml">
            <xpath expr="//notebook/page[@name='pricelist_rules']//field[@name='item_ids']//list//field[@name='date_end']" position="after">
                <button name="action_view_update_logs"
                        string="View History"
                        type="object"
                        icon="fa-history"
                        />
            </xpath>
        </field>
    </record>
</odoo>
